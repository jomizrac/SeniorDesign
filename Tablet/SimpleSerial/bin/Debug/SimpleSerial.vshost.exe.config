<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- NOTE: Do not enter raw AWS credentials in this version-controlled App.config file!
    They belong in the following local-only credentials file: -->
    <add key="AWSProfilesLocation" value="C:\\ShelfRokr\\AWSConfig\\credentials"/>
    <add key="AWSRegion" value="us-east-1"/>

    <!-- Arduino parser. -->
    <add key="defaultPort" value="COM4"/>
    <add key="baudRate" value="9600"/>

    <add key="videoConfig" value="C:\\ShelfRokr\\Config\\videoConfig.json"/>

    <!-- S3 bucket settings. -->
    <add key="productVideoBucket" value="shelfrokr-product-videos"/>
    <add key="videoDirectory" value="C:\\ShelfRokr\\ProductVideos\\"/>
    <add key="videoFileExtension" value=".wmv"/>

    <add key="shelfInventoryFile" value="C:\\ShelfRokr\\Data\\products.json"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
